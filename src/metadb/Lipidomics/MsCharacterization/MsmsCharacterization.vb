        If spectrum Is Nothing OrElse spectrum.Count = 0 Then Return Nothing
        If maxSphCarbon > totalCarbon Then maxSphCarbon = totalCarbon
        If maxSphDoubleBond > totalDoubleBond Then maxSphDoubleBond = totalDoubleBond
