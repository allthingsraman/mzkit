                                Dim nl_SN1andH2O = nl_SN1 - H2O
                                Dim nl_SN2andH2O = nl_SN2 - H2O

                                Dim query = New List(Of SpectrumPeak) From {
                                    New SpectrumPeak() With {
                                        .mz = nl_SN1,
