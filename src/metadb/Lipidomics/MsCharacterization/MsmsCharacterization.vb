            Dim threshold = 1.0
            Dim diagnosticMz = theoreticalMz - 260.029722
            Dim isClassIonFound = LipidMsmsCharacterizationUtility.isDiagnosticFragmentExist(spectrum, ms2Tolerance, diagnosticMz, threshold)
