                ' seek [[M-C6H10O5-H]-
                Dim threshold1 = 1.0
                Dim diagnosticMz1 = diagnosticMz - 162.052833

                Dim isClassIonFound = LipidMsmsCharacterizationUtility.isDiagnosticFragmentExist(spectrum, ms2Tolerance, diagnosticMz1, threshold1)
