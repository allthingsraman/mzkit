                                Dim molecule = LipidMsmsCharacterizationUtility.getNacylphospholipidMoleculeObjAsLevel2("LNAPS", LbmClass.LNAPS, sn1Carbon, sn1Double, sn2Carbon, sn2Double, averageIntensity)
                                candidates.Add(molecule)
