                            Dim molecule = LipidMsmsCharacterizationUtility.getCeramideoxMoleculeObjAsLevel2("HexCer", LbmClass.HexCer_AP, "t", sphCarbon, sphDouble, acylCarbon, acylDouble, 1, averageIntensity)
