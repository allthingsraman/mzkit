                    'if (acylCarbon < minSphCarbon) { break; }
                    Dim acylDouble = totalDoubleBond - sphDouble

                    Dim sph1 = diagnosticMz - LipidMsmsCharacterizationUtility.acylCainMass(acylCarbon, acylDouble) + HydrogenMass
