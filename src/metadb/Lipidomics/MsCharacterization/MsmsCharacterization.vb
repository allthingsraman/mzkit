                            Dim molecule = LipidMsmsCharacterizationUtility.getEsterceramideMoleculeObjAsLevel2_1("HexCer", LbmClass.HexCer_EOS, "d", sphCarbon, sphDouble, acylCarbon, acylDouble, averageIntensity)
                            candidates.Add(molecule)
